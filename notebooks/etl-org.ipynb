    "def clean_number(x):\n",
    "    url = \"http://localhost:30888/lab/entities/parse\"\n",
    "    body = {\"user_input\": x, \"entities\": \"NumeroCelular\"}\n",
    "\n",
    "    response = requests.post(url, json=body)\n",
    "    if response.status_code == 200:\n",
    "        return response.json()[\"NumeroCelular\"]\n",
    "    else:\n",
    "        return None"
